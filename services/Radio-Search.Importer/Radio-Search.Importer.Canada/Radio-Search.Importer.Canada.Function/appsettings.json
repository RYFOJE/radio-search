{
  "Keyvault-Importer": "https://rds-importers.vault.azure.net/",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "None",
      "Microsoft.EntityFrameworkCore.Database.Connection": "None",
      "Microsoft.EntityFrameworkCore.Infrastructure": "None",
      "Microsoft.EntityFrameworkCore.Query": "None",
      "System.Net.Http.HttpClient": "Warning",
      "System.Net.Http.HttpClient.CallInvokerExtractor.ClientHandler": "None",
      "System.Net.Http.HttpClient.CallInvokerExtractor.LogicalHandler": "None"
    }
  },
  "AppConfig": {
    "Common": {
      "URL": "https://rds-config.azconfig.io",
      "Sentinel": "Common:Sentinel",
      "RefreshInterval": 15
    },
    "Importer": {
      "URL": "https://rds-importer.azconfig.io",
      "Sentinel": "Sentinel",
      "RefreshInterval": 5,
      "Prefix": "Canada"
    }
  },
  "ApplicationInsights": {
    "ApplicationName": "Importer.Canada"
  },
  "BlobStorage": {
    //"URL": "https://rdsimporter.blob.core.windows.net/"
    "URL": "UseDevelopmentStorage=true"
  },
  "ConnectionStrings": {
    "CanadaImporter": "Server=tcp:rds-full-db.database.windows.net,1433;Initial Catalog=full-db;Persist Security Info=False;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;Authentication=Active Directory Default;",
    "CanadaImporterServiceBus": "https://rds-importers.servicebus.windows.net:443/"
  },
  "ServiceBusNames": {
    "DownloadCompleteQueue": "DownloadComplete"
  }
}
