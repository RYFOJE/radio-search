{
  "version": "2.0",
  "functionTimeout": "00:05:00",
  "logging": {
    "logLevel": {
      "Default": "Trace",
      "Function": "Trace",
      "Microsoft": "Trace",
      "Microsoft.Azure.Functions.Worker": "Trace",
      "Microsoft.Azure.Functions.Worker.Http": "Trace",
      "Microsoft.Azure.WebJobs": "Trace",
      "Azure.Messaging.ServiceBus": "Trace",
      "Azure.Storage": "Trace",
      "Microsoft.EntityFrameworkCore": "Trace"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "serviceBus": {
      "prefetchCount": 0,
      "messageHandlerOptions": {
        "maxConcurrentCalls": 1
      }
    }
  }
}